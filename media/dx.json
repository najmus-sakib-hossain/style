{
    "name": "dx",
    "version": "0.0.1",
    "title": "Enhanced Developing Experience",
    "description": "Orchestrate don't just own your code",
    "author": "essensefromexistence",

    "language": "auto-detected",
    "runtime": "auto-detected",
    "compiler": "auto-detected",
    "bundler": "auto-detected",
    "interpreter": "auto-detected",
    "packageManager": "auto-detected",
    "framework": "auto-detected",

    "forge": "https://dx.vercel.app/essensefromexistence/dx",
    "style": {
        "path": "@/style",
        "engine": [
            "automic",
            "enhanced",
            "logical"
        ],
        "themes": [
            "dx",
            "vercel",
            "claude"
        ]
    },
    "ui": {
        "path": "@/components/ui",
        "items": [
            "button",
            "card",
            "modal",
            "navbar",
            "footer"
        ]
    },
    "check": {
        "lint": {},
        "format": {},
        "fix": {}
    },
    "media": {
        "images": {
            "path": "@/public/images/*",
            "items": [
                "dummy1.jpg",
                "dummy2.png"
            ]
        },
        "videos": {
            "path": "@/public/videos/*",
            "items": [
                "dummy1.mp4"
            ]
        },
        "sounds": {
            "path": "@/public/sounds/*",
            "items": [
                "dummy1.wav"
            ]
        },
        "assets": {
            "path": "@/public/assets/*",
            "items": [
                "dummy1.asset"
            ]
        }
    },
    "internationalization": {
        "locales": {
            "path": "@/locales",
            "default": "en-US",
            "dev": [
                "en-US"
            ],
            "prod": [
                "all"
            ]
        },
        "ttses": {
            "path": "./ttses",
            "default": "en-US",
            "dev": [
                "en-US",
                "bn-BD"
            ],
            "prod": [
                "all"
            ]
        }
    },
    "icon": {
        "path": "@/components/icons",
        "pack": "lucide-react",
        "variant": "default"
    },
    "font": {
        "path": "@/font",
        "default": "Inter",
        "primary": "Manrope",
        "secondary": "Roboto Mono"
    },

    "dependencies": {},
    "devDependencies": {},
    "peerDependencies": {},

    "workspace": {
        "frontend/www": {},
        "frontend/mobile": {}
    },
    "ide": {
        "vscode": {},
        "vim": {},
        "gitpod": {},
        "github-codespace": {},
        "replit": {},
        "firebase-studio": {},
        "cursor": {},
        "windsruff": {},
        "stackblitz": {}
    }
}