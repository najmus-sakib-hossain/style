{
    "name": "dx",
    "version": "0.0.1",
    "title": "Enhanced Developing Experience",
    "description": "Orchestrate don't just own your code",
    "author": "essensefromexistence",
    "languages": [
        {
            "name": "javascript/typescript",
            "runtime": "bun",
            "compiler": "tsc",
            "bundler": "vite",
            "packageManager": "bun",
            "framework": "react"
        },
        {
            "name": "python",
            "runtime": "cpython",
            "packageManager": "uv",
            "framework": "django"
        },
        {
            "name": "rust",
            "runtime": "native",
            "compiler": "rustc",
            "packageManager": "cargo"
        }
    ],
    "forge": {
        "repository": "https://dx.vercel.app/essensefromexistence/dx",
        "container": "none",
        "ci/cd": "none",
        "tasks": "none",
        "items": [
            "cli",
            "docs",
            "examples",
            "packages",
            "scripts",
            "style",
            "tests"
        ]
    },
    "style": {
        "path": "@/style",
        "engine": [
            "automic",
            "enhanced",
            "logical"
        ],
        "themes": [
            "dx",
            "vercel",
            "claude"
        ]
    },
    "ui": {
        "path": "@/components/ui",
        "items": [
            "button",
            "card",
            "modal",
            "navbar",
            "footer"
        ]
    },
    "check": {
        "lint": {},
        "format": {},
        "fix": {}
    },
    "media": {
        "images": {
            "path": "@/public/images/*",
            "items": [
                "dummy1.jpg",
                "dummy2.png"
            ]
        },
        "videos": {
            "path": "@/public/videos/*",
            "items": [
                "dummy1.mp4"
            ]
        },
        "sounds": {
            "path": "@/public/sounds/*",
            "items": [
                "dummy1.wav"
            ]
        },
        "assets": {
            "path": "@/public/assets/*",
            "items": [
                "dummy1.asset"
            ]
        }
    },
    "internationalization": {
        "locales": {
            "path": "@/locales",
            "default": "en-US",
            "dev": [
                "en-US"
            ],
            "prod": [
                "all"
            ]
        },
        "ttses": {
            "path": "./ttses",
            "default": "en-US",
            "dev": [
                "en-US",
                "bn-BD"
            ],
            "prod": [
                "all"
            ]
        }
    },
    "icon": {
        "path": "@/components/icons",
        "pack": "lucide-react",
        "variant": "default"
    },
    "font": {
        "path": "@/font",
        "default": "Inter",
        "primary": "Manrope",
        "secondary": "Roboto Mono"
    },
    "dependencies": {},
    "devDependencies": {},
    "peerDependencies": {},
    "workspace": {
        "frontend/www": {},
        "frontend/mobile": {}
    },
    "ide": {
        "vscode": {},
        "vim": {},
        "gitpod": {},
        "github-codespace": {},
        "replit": {},
        "firebase-studio": {},
        "cursor": {},
        "windsruff": {},
        "stackblitz": {}
    }
}
